/* pages/feedback/feedback.wxss */
page{
  background-color: #eeeeee;
}
.fb_main{
  padding: 20rpx;
  color: #666;
}
.fb_tips{
  display: flex;
  flex-wrap: wrap;
  padding: 0;
}
text{
  width: 28%;
  padding: 10rpx;
  text-align: center;
  background-color: #fff;
  margin: 16rpx 8rpx;
}
.fb_content{
  background-color: #fff;
  margin-top: 20rpx;
}
textarea{
  padding: 10rpx;
}
.fb_content_tools{
  display: flex;
  flex-wrap: wrap;
}
.fb_content_tools button{
  margin: 0;
  width: 90rpx;
  height: 90rpx;
  font-size: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  padding: 0;
  color: #ccc;
  margin-bottom: 20rpx;
  
}
.up_img_item{
  margin-left: 28rpx;
}
.form_btn_wrap{
  margin-top: 20rpx;
  display: flex;
  justify-content:flex-end;
}
.form_btn_wrap button{
  width: 28%;
  margin: 0;
}
.ts_reason{
  background-color: #fff;
  width: 100%;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
input{
  /* border: 2rpx solid #666; */
  height: 40rpx;
  border-radius: 10rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  background-color:#fff;
}
