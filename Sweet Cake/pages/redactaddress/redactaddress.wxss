/* pages/editaddress/editaddress.wxss */
page{
  background-color: #f5f5f5;
}
.content{
  margin: 6rpx 20rpx;
  border-radius: 10rpx;
}
.label-txt{
  width: 130rpx;
  font-size: 26rpx;
  color: #333333;
}
.input-txt{
  font-size: 26rpx;
}
.input-placeholder{
  font-size: 26rpx;
  color: #cdcdcd;
}
.check-default{
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  margin-left: 20rpx;
  font-size: 24rpx;
}
.check-default icon {
  margin-right: 10rpx;
}
.add-address{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f5f5f5;
}
.add-address .txt{
  width: 60vw;
  height: 75rpx;
  margin-bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 2rpx solid #ff6700;
  background-origin: 5rpx;
  font-size: 26rpx;
  color: #ff6700;
}






